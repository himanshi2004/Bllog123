                                <div class="col-md-9">
                                  <textarea name="category" rows="5" class="form-control" placeholder="Category" required></textarea>
                                </div>
                            </div>
                         
                            <div class="form-group form-row">
                                <label class="col-md-3"></label>
                                <div class="col-md-9">
                                <button class="btn btn-primary">Save</button>
                                </div>
